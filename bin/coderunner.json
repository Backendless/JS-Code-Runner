{
  "managementHttpPort": 2992,
  "workers": {
    "cache": true,
    "limit": 20,
    "concurrent": 4,
    "heartbeat": {
      "interval": 5,
      "timeout": 10
    }
  },
  "backendless": {
    "msgBroker": {
      "host": "cl.backendless.com",
      "port": 6379,
      "max_attempts": 5
    }
  },
  "app": {
    "id": "",
    "apiKey": "",
    "exclude": [
      "+(git|.idea|lib|test|docs|bin|deploy)/**",
      "+(LICENSE|*.md|*.json|.*)",
      "node_modules/**",
      "examples/services/wsdl-proxy/**"
    ]
  },
  "sandbox": true,
  "verbose": false
}